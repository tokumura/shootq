<h1>Listing result_seconds</h1>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Group name</th>
      <th>Countries</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @result_seconds.each do |result_second| %>
      <tr>
        <td><%= result_second.group_name %></td>
        <td><%= result_second.countries %></td>
        <td><%= link_to 'Show', result_second %></td>
        <td><%= link_to 'Edit', edit_result_second_path(result_second) %></td>
        <td><%= link_to 'Destroy', result_second, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result second', new_result_second_path %>
