<h1>Listing result_firsts</h1>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Match date</th>
      <th>Dayofweek</th>
      <th>Match time</th>
      <th>Group name</th>
      <th>Match code</th>
      <th>Score l</th>
      <th>Score r</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @result_firsts.each do |result_first| %>
      <tr>
        <td><%= result_first.match_date %></td>
        <td><%= result_first.dayofweek %></td>
        <td><%= result_first.match_time %></td>
        <td><%= result_first.group_name %></td>
        <td><%= result_first.match_code %></td>
        <td><%= result_first.score_l %></td>
        <td><%= result_first.score_r %></td>
        <td><%= link_to 'Show', result_first %></td>
        <td><%= link_to 'Edit', edit_result_first_path(result_first) %></td>
        <td><%= link_to 'Destroy', result_first, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result first', new_result_first_path %>
